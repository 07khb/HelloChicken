			prefHeight="123.0" prefWidth="244.0" />
		<TextField fx:id="phoneNum" layoutX="59.0" layoutY="369.0"
			prefHeight="25.0" prefWidth="167.0" promptText="예) 01012345678" />
		<Button fx:id="btnJoin" layoutX="119.0" layoutY="435.0"
			mnemonicParsing="false" onAction="#handleBtnJoinAction"
			prefHeight="33.0" prefWidth="80.0"
			style="-fx-background-color: orange;" text="가입하기" textFill="WHITE">
			<font>
				<Font size="13.0" />
			</font>
		</Button>
	</children>
</AnchorPane>
